<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="brainGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4facfe;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00f2fe;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#bgGradient)"/>
  
  <!-- Circuit pattern background -->
  <g opacity="0.1">
    <path d="M50 50 L150 50 L150 100 L250 100" stroke="#fff" stroke-width="2" fill="none"/>
    <path d="M300 150 L400 150 L400 200 L500 200" stroke="#fff" stroke-width="2" fill="none"/>
    <path d="M550 250 L650 250 L650 300 L750 300" stroke="#fff" stroke-width="2" fill="none"/>
    <circle cx="150" cy="50" r="4" fill="#fff"/>
    <circle cx="250" cy="100" r="4" fill="#fff"/>
    <circle cx="400" cy="150" r="4" fill="#fff"/>
    <circle cx="500" cy="200" r="4" fill="#fff"/>
    <circle cx="650" cy="250" r="4" fill="#fff"/>
  </g>
  
  <!-- Brain illustration -->
  <g transform="translate(200, 80)">
    <path d="M100 50 C120 30, 160 30, 180 50 C200 70, 200 110, 180 130 C160 150, 120 150, 100 130 C80 110, 80 70, 100 50 Z" 
          fill="url(#brainGradient)" opacity="0.8"/>
    <path d="M110 60 C125 45, 155 45, 170 60 C185 75, 185 105, 170 120 C155 135, 125 135, 110 120 C95 105, 95 75, 110 60 Z" 
          fill="none" stroke="#fff" stroke-width="2" opacity="0.6"/>
    
    <!-- Neural network nodes -->
    <circle cx="120" cy="80" r="3" fill="#fff" opacity="0.8"/>
    <circle cx="140" cy="70" r="3" fill="#fff" opacity="0.8"/>
    <circle cx="160" cy="85" r="3" fill="#fff" opacity="0.8"/>
    <circle cx="130" cy="100" r="3" fill="#fff" opacity="0.8"/>
    <circle cx="150" cy="110" r="3" fill="#fff" opacity="0.8"/>
    
    <!-- Connections -->
    <line x1="120" y1="80" x2="140" y2="70" stroke="#fff" stroke-width="1" opacity="0.6"/>
    <line x1="140" y1="70" x2="160" y2="85" stroke="#fff" stroke-width="1" opacity="0.6"/>
    <line x1="120" y1="80" x2="130" y2="100" stroke="#fff" stroke-width="1" opacity="0.6"/>
    <line x1="160" y1="85" x2="150" y2="110" stroke="#fff" stroke-width="1" opacity="0.6"/>
  </g>
  
  <!-- Medical cross -->
  <g transform="translate(500, 120)">
    <rect x="45" y="20" width="10" height="60" fill="#fff" opacity="0.9"/>
    <rect x="20" y="45" width="60" height="10" fill="#fff" opacity="0.9"/>
  </g>
  
  <!-- Data visualization elements -->
  <g transform="translate(100, 250)">
    <rect x="0" y="20" width="8" height="40" fill="#4facfe" opacity="0.7"/>
    <rect x="15" y="10" width="8" height="50" fill="#4facfe" opacity="0.7"/>
    <rect x="30" y="25" width="8" height="35" fill="#4facfe" opacity="0.7"/>
    <rect x="45" y="5" width="8" height="55" fill="#4facfe" opacity="0.7"/>
    <rect x="60" y="15" width="8" height="45" fill="#4facfe" opacity="0.7"/>
  </g>
  
  <!-- Title overlay -->
  <text x="400" y="350" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="24" font-weight="bold" opacity="0.9">
    AI in Healthcare
  </text>
</svg>