<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3498db;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2980b9;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="rocketGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e74c3c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#c0392b;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="moneyGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f1c40f;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f39c12;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="flameGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ff6b35;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f7931e;stop-opacity:0.8" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#skyGradient)"/>
  
  <!-- City skyline -->
  <g opacity="0.3">
    <rect x="0" y="300" width="60" height="100" fill="#2c3e50"/>
    <rect x="80" y="280" width="50" height="120" fill="#34495e"/>
    <rect x="150" y="320" width="40" height="80" fill="#2c3e50"/>
    <rect x="210" y="290" width="55" height="110" fill="#34495e"/>
    <rect x="280" y="310" width="45" height="90" fill="#2c3e50"/>
    <rect x="350" y="270" width="60" height="130" fill="#34495e"/>
    <rect x="430" y="300" width="50" height="100" fill="#2c3e50"/>
    <rect x="500" y="285" width="55" height="115" fill="#34495e"/>
    <rect x="580" y="295" width="45" height="105" fill="#2c3e50"/>
    <rect x="650" y="275" width="50" height="125" fill="#34495e"/>
    <rect x="720" y="305" width="80" height="95" fill="#2c3e50"/>
    
    <!-- Building windows -->
    <rect x="10" y="310" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
    <rect x="25" y="320" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
    <rect x="40" y="315" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
    <rect x="90" y="290" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
    <rect x="105" y="300" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
    <rect x="220" y="300" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
    <rect x="235" y="310" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
  </g>
  
  <!-- Startup rocket -->
  <g transform="translate(350, 150)">
    <!-- Rocket body -->
    <path d="M0 100 L20 100 L25 80 L25 20 L12.5 0 L0 20 L0 80 Z" fill="url(#rocketGradient)"/>
    
    <!-- Rocket nose -->
    <path d="M0 20 L12.5 0 L25 20" fill="#95a5a6"/>
    
    <!-- Rocket fins -->
    <path d="M0 80 L-10 90 L0 100" fill="#34495e"/>
    <path d="M25 80 L35 90 L25 100" fill="#34495e"/>
    
    <!-- Rocket window -->
    <circle cx="12.5" cy="40" r="8" fill="#87ceeb" opacity="0.8"/>
    <circle cx="12.5" cy="40" r="5" fill="#3498db" opacity="0.6"/>
    
    <!-- Rocket flames -->
    <ellipse cx="5" cy="105" rx="3" ry="8" fill="url(#flameGradient)"/>
    <ellipse cx="12.5" cy="110" rx="4" ry="12" fill="url(#flameGradient)"/>
    <ellipse cx="20" cy="105" rx="3" ry="8" fill="url(#flameGradient)"/>
    
    <!-- Startup logo on rocket -->
    <rect x="8" y="60" width="9" height="9" fill="#fff" opacity="0.9"/>
    <text x="12.5" y="67" text-anchor="middle" fill="#e74c3c" font-family="Arial, sans-serif" font-size="8" font-weight="bold">S</text>
  </g>
  
  <!-- Money/Investment symbols -->
  <g transform="translate(100, 200)">
    <!-- Dollar bills -->
    <rect x="0" y="0" width="40" height="20" fill="url(#moneyGradient)" stroke="#27ae60" stroke-width="1"/>
    <text x="20" y="12" text-anchor="middle" fill="#27ae60" font-family="Arial, sans-serif" font-size="12" font-weight="bold">$</text>
    
    <rect x="20" y="10" width="40" height="20" fill="url(#moneyGradient)" stroke="#27ae60" stroke-width="1"/>
    <text x="40" y="22" text-anchor="middle" fill="#27ae60" font-family="Arial, sans-serif" font-size="12" font-weight="bold">$</text>
    
    <rect x="40" y="20" width="40" height="20" fill="url(#moneyGradient)" stroke="#27ae60" stroke-width="1"/>
    <text x="60" y="32" text-anchor="middle" fill="#27ae60" font-family="Arial, sans-serif" font-size="12" font-weight="bold">$</text>
    
    <!-- Coins -->
    <circle cx="100" cy="10" r="8" fill="#f39c12" stroke="#e67e22" stroke-width="2"/>
    <text x="100" y="14" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">¢</text>
    
    <circle cx="115" cy="20" r="8" fill="#f39c12" stroke="#e67e22" stroke-width="2"/>
    <text x="115" y="24" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">¢</text>
    
    <circle cx="130" cy="30" r="8" fill="#f39c12" stroke="#e67e22" stroke-width="2"/>
    <text x="130" y="34" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">¢</text>
  </g>
  
  <!-- Investors/VCs -->
  <g transform="translate(500, 250)">
    <!-- Investor 1 -->
    <circle cx="0" cy="0" r="15" fill="#34495e"/>
    <circle cx="0" cy="-5" r="8" fill="#f4d1ae"/>
    <rect x="-8" y="5" width="16" height="20" fill="#2c3e50"/>
    <rect x="-6" y="7" width="12" height="16" fill="#fff"/>
    <rect x="-4" y="9" width="8" height="2" fill="#e74c3c"/>
    
    <!-- Investor 2 -->
    <circle cx="40" cy="0" r="15" fill="#34495e"/>
    <circle cx="40" cy="-5" r="8" fill="#f4d1ae"/>
    <rect x="32" y="5" width="16" height="20" fill="#2c3e50"/>
    <rect x="34" y="7" width="12" height="16" fill="#fff"/>
    <rect x="36" y="9" width="8" height="2" fill="#3498db"/>
    
    <!-- Investor 3 -->
    <circle cx="80" cy="0" r="15" fill="#34495e"/>
    <circle cx="80" cy="-5" r="8" fill="#f4d1ae"/>
    <rect x="72" y="5" width="16" height="20" fill="#2c3e50"/>
    <rect x="74" y="7" width="12" height="16" fill="#fff"/>
    <rect x="76" y="9" width="8" height="2" fill="#27ae60"/>
    
    <!-- Briefcases -->
    <rect x="-5" y="25" width="10" height="6" fill="#8b4513"/>
    <rect x="35" y="25" width="10" height="6" fill="#8b4513"/>
    <rect x="75" y="25" width="10" height="6" fill="#8b4513"/>
  </g>
  
  <!-- Growth chart -->
  <g transform="translate(550, 80)">
    <!-- Chart background -->
    <rect x="0" y="0" width="200" height="120" fill="#fff" opacity="0.9" stroke="#2c3e50" stroke-width="2"/>
    
    <!-- Chart title -->
    <text x="100" y="15" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Growth Projection</text>
    
    <!-- Chart axes -->
    <line x1="20" y1="100" x2="180" y2="100" stroke="#2c3e50" stroke-width="2"/>
    <line x1="20" y1="100" x2="20" y2="30" stroke="#2c3e50" stroke-width="2"/>
    
    <!-- Growth line -->
    <path d="M20 90 L50 80 L80 65 L110 45 L140 30 L170 20" stroke="#e74c3c" stroke-width="3" fill="none"/>
    
    <!-- Data points -->
    <circle cx="20" cy="90" r="3" fill="#e74c3c"/>
    <circle cx="50" cy="80" r="3" fill="#e74c3c"/>
    <circle cx="80" cy="65" r="3" fill="#e74c3c"/>
    <circle cx="110" cy="45" r="3" fill="#e74c3c"/>
    <circle cx="140" cy="30" r="3" fill="#e74c3c"/>
    <circle cx="170" cy="20" r="3" fill="#e74c3c"/>
    
    <!-- Labels -->
    <text x="20" y="110" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="8">Q1</text>
    <text x="80" y="110" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="8">Q2</text>
    <text x="140" y="110" text-anchor="middle" fill="#2c3e50" font-family="Arial, sans-serif" font-size="8">Q3</text>
    
    <text x="15" y="95" text-anchor="end" fill="#2c3e50" font-family="Arial, sans-serif" font-size="8">1M</text>
    <text x="15" y="65" text-anchor="end" fill="#2c3e50" font-family="Arial, sans-serif" font-size="8">5M</text>
    <text x="15" y="35" text-anchor="end" fill="#2c3e50" font-family="Arial, sans-serif" font-size="8">10M</text>
  </g>
  
  <!-- Investment flow arrows -->
  <g opacity="0.8">
    <path d="M250 250 Q300 230, 350 250" stroke="#27ae60" stroke-width="4" fill="none"/>
    <polygon points="350,250 345,245 345,255" fill="#27ae60"/>
    
    <path d="M450 200 Q480 180, 510 200" stroke="#27ae60" stroke-width="4" fill="none"/>
    <polygon points="510,200 505,195 505,205" fill="#27ae60"/>
    
    <!-- Investment amounts -->
    <text x="300" y="220" text-anchor="middle" fill="#27ae60" font-family="Arial, sans-serif" font-size="14" font-weight="bold">$2M</text>
    <text x="480" y="170" text-anchor="middle" fill="#27ae60" font-family="Arial, sans-serif" font-size="14" font-weight="bold">$5M</text>
  </g>
  
  <!-- Lightbulb (idea) -->
  <g transform="translate(50, 50)">
    <circle cx="0" cy="0" r="20" fill="#f1c40f" opacity="0.8"/>
    <rect x="-5" y="15" width="10" height="8" fill="#95a5a6"/>
    <rect x="-3" y="23" width="6" height="3" fill="#7f8c8d"/>
    
    <!-- Light rays -->
    <line x1="-25" y1="-15" x2="-30" y2="-20" stroke="#f1c40f" stroke-width="2"/>
    <line x1="25" y1="-15" x2="30" y2="-20" stroke="#f1c40f" stroke-width="2"/>
    <line x1="-25" y1="15" x2="-30" y2="20" stroke="#f1c40f" stroke-width="2"/>
    <line x1="25" y1="15" x2="30" y2="20" stroke="#f1c40f" stroke-width="2"/>
    <line x1="0" y1="-25" x2="0" y2="-30" stroke="#f1c40f" stroke-width="2"/>
    <line x1="-30" y1="0" x2="-35" y2="0" stroke="#f1c40f" stroke-width="2"/>
    <line x1="30" y1="0" x2="35" y2="0" stroke="#f1c40f" stroke-width="2"/>
  </g>
  
  <!-- Funding stages -->
  <g transform="translate(50, 350)" opacity="0.9">
    <rect x="0" y="0" width="60" height="30" fill="#3498db" rx="5"/>
    <text x="30" y="18" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">Seed</text>
    
    <rect x="80" y="0" width="60" height="30" fill="#e67e22" rx="5"/>
    <text x="110" y="18" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">Series A</text>
    
    <rect x="160" y="0" width="60" height="30" fill="#9b59b6" rx="5"/>
    <text x="190" y="18" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">Series B</text>
    
    <rect x="240" y="0" width="60" height="30" fill="#e74c3c" rx="5"/>
    <text x="270" y="18" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="10" font-weight="bold">IPO</text>
  </g>
  
  <!-- Title -->
  <text x="400" y="30" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="24" font-weight="bold" opacity="0.9">
    Startup Investment Surge
  </text>
</svg>