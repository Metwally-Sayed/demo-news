<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="medicalBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c5530;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a3a1f;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="cellGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#4ade80;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#22c55e;stop-opacity:0.4" />
    </radialGradient>
    <radialGradient id="microscopeLight" cx="50%" cy="30%" r="40%">
      <stop offset="0%" style="stop-color:#fff;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#fff;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#medicalBg)"/>
  
  <!-- Microscope light effect -->
  <ellipse cx="400" cy="120" rx="200" ry="100" fill="url(#microscopeLight)"/>
  
  <!-- Microscope -->
  <g transform="translate(100, 150)">
    <!-- Base -->
    <rect x="40" y="180" width="80" height="20" rx="5" fill="#333"/>
    <rect x="50" y="160" width="60" height="20" fill="#444"/>
    
    <!-- Arm -->
    <rect x="75" y="80" width="10" height="80" fill="#555"/>
    <rect x="70" y="75" width="20" height="10" fill="#555"/>
    
    <!-- Eyepiece -->
    <circle cx="80" cy="70" r="8" fill="#333"/>
    <circle cx="80" cy="70" r="5" fill="#000"/>
    
    <!-- Objective -->
    <rect x="75" y="140" width="10" height="20" fill="#666"/>
    <circle cx="80" cy="160" r="6" fill="#333"/>
  </g>
  
  <!-- DNA helix -->
  <g transform="translate(500, 100)">
    <path d="M0 0 Q20 20, 0 40 Q-20 60, 0 80 Q20 100, 0 120" stroke="#4ade80" stroke-width="4" fill="none" opacity="0.8"/>
    <path d="M40 0 Q20 20, 40 40 Q60 60, 40 80 Q20 100, 40 120" stroke="#4ade80" stroke-width="4" fill="none" opacity="0.8"/>
    
    <!-- DNA base pairs -->
    <line x1="0" y1="10" x2="40" y2="10" stroke="#22c55e" stroke-width="2" opacity="0.7"/>
    <line x1="0" y1="30" x2="40" y2="30" stroke="#22c55e" stroke-width="2" opacity="0.7"/>
    <line x1="0" y1="50" x2="40" y2="50" stroke="#22c55e" stroke-width="2" opacity="0.7"/>
    <line x1="0" y1="70" x2="40" y2="70" stroke="#22c55e" stroke-width="2" opacity="0.7"/>
    <line x1="0" y1="90" x2="40" y2="90" stroke="#22c55e" stroke-width="2" opacity="0.7"/>
    <line x1="0" y1="110" x2="40" y2="110" stroke="#22c55e" stroke-width="2" opacity="0.7"/>
  </g>
  
  <!-- Cell structures -->
  <g opacity="0.7">
    <!-- Healthy cells -->
    <circle cx="300" cy="200" r="25" fill="url(#cellGradient)" stroke="#4ade80" stroke-width="2"/>
    <circle cx="350" cy="180" r="20" fill="url(#cellGradient)" stroke="#4ade80" stroke-width="2"/>
    <circle cx="280" cy="250" r="22" fill="url(#cellGradient)" stroke="#4ade80" stroke-width="2"/>
    
    <!-- Cell nuclei -->
    <circle cx="300" cy="200" r="8" fill="#22c55e" opacity="0.8"/>
    <circle cx="350" cy="180" r="6" fill="#22c55e" opacity="0.8"/>
    <circle cx="280" cy="250" r="7" fill="#22c55e" opacity="0.8"/>
    
    <!-- Cancer cells (different color) -->
    <circle cx="450" cy="220" r="18" fill="#ef4444" opacity="0.6" stroke="#dc2626" stroke-width="2"/>
    <circle cx="480" cy="190" r="15" fill="#ef4444" opacity="0.6" stroke="#dc2626" stroke-width="2"/>
    
    <!-- Treatment targeting -->
    <path d="M420 200 L445 215" stroke="#fbbf24" stroke-width="3" opacity="0.8"/>
    <path d="M425 205 L450 190" stroke="#fbbf24" stroke-width="3" opacity="0.8"/>
    <circle cx="450" cy="215" r="3" fill="#fbbf24"/>
    <circle cx="455" cy="195" r="3" fill="#fbbf24"/>
  </g>
  
  <!-- Medical cross -->
  <g transform="translate(650, 80)">
    <rect x="20" y="5" width="8" height="40" fill="#4ade80" opacity="0.9"/>
    <rect x="5" y="20" width="38" height="8" fill="#4ade80" opacity="0.9"/>
  </g>
  
  <!-- Research data/charts -->
  <g transform="translate(600, 250)">
    <rect x="0" y="30" width="6" height="20" fill="#4ade80" opacity="0.7"/>
    <rect x="10" y="20" width="6" height="30" fill="#4ade80" opacity="0.7"/>
    <rect x="20" y="25" width="6" height="25" fill="#4ade80" opacity="0.7"/>
    <rect x="30" y="15" width="6" height="35" fill="#4ade80" opacity="0.7"/>
    <rect x="40" y="10" width="6" height="40" fill="#4ade80" opacity="0.7"/>
  </g>
  
  <!-- Molecular structures -->
  <g transform="translate(50, 50)" opacity="0.5">
    <circle cx="20" cy="20" r="4" fill="#4ade80"/>
    <circle cx="35" cy="15" r="4" fill="#22c55e"/>
    <circle cx="25" cy="35" r="4" fill="#16a34a"/>
    <line x1="20" y1="20" x2="35" y2="15" stroke="#4ade80" stroke-width="2"/>
    <line x1="20" y1="20" x2="25" y2="35" stroke="#4ade80" stroke-width="2"/>
  </g>
  
  <!-- Title -->
  <text x="400" y="370" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="24" font-weight="bold" opacity="0.9">
    Cancer Research
  </text>
</svg>